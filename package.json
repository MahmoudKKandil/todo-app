{
  "name": "101-app",
  "version": "1.0.8",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prettify": "grep '!' .prettierignore | cut --delimiter='!' -f2 | xargs -I {} bash -c 'yarn prettier --write ./{}'",
    "test": "jest",
    "dev": "nodemon src/index.js",
    "preversion": "yarn prettify && yarn test",
    "postversion": "git push --atomic origin master $(echo $(yarn version) | awk '{print $7}')"
  },
  "dependencies": {
    "express": "^4.17.1",
    "mysql": "^2.17.1",
    "sqlite3": "^5.0.0",
    "uuid": "^3.3.3",
    "wait-port": "^0.2.2"
  },
  "resolutions": {
    "ansi-regex": "5.0.1"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true
  },
  "devDependencies": {
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "prettier": "^1.18.2"
  }
}
